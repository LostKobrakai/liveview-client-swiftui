              "type" : "reference"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "and published on ",
              "type" : "text"
            },
            {
              "identifier" : "https:\/\/hexdocs.pm",
              "isActive" : true,
              "type" : "reference"
