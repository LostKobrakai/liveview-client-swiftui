      "content" : [
        {
          "anchor" : "Overview",
          "level" : 2,
          "text" : "Overview",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "The LiveViewNative Swift package lets you use Phoenix LiveView to build native iOS apps with SwiftUI.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "anchor" : "Installation",
          "level" : 2,
          "text" : "Installation",
          "type" : "heading"
        },
        {
          "items" : [
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "In Xcode, select ",
                      "type" : "text"
                    },
                    {
                      "inlineContent" : [
                        {
                          "text" : "File → Add Packages…",
                          "type" : "text"
                        }
                      ],
                      "type" : "emphasis"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "Enter the package URL ",
                      "type" : "text"
                    },
                    {
                      "code" : "https:\/\/github.com\/liveview-native\/liveview-client-swiftui",
                      "type" : "codeVoice"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "Select ",
                      "type" : "text"
                    },
                    {
                      "inlineContent" : [
                        {
                          "text" : "Add Package",
                          "type" : "text"
                        }
                      ],
                      "type" : "emphasis"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            }
          ],
          "type" : "orderedList"
        },
        {
          "anchor" : "Usage",
          "level" : 2,
          "text" : "Usage",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "Create a ",
              "type" : "text"
            },
            {
              "code" : "LiveSessionCoordinator",
              "type" : "codeVoice"
            },
            {
              "text" : " and pass in the URL to your Phoenix server. Then use ",
              "type" : "text"
            },
            {
              "code" : "LiveView",
              "type" : "codeVoice"
            },
            {
              "text" : " to connect to that session.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "import SwiftUI",
            "import LiveViewNative",
            "",
            "struct ContentView: View {",
            "    @StateObject private var session = LiveSessionCoordinator(URL(string: \"http:\/\/localhost:4000\")!)",
            "",
            "    var body: some View {",
            "        LiveView(session: session)",
            "    }",
            "}"
          ],
          "syntax" : "swift",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "Now when you start up your app, the LiveView will automatically connect and serve your native app.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "anchor" : "Elixir-Library",
          "level" : 2,
          "text" : "Elixir Library",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "This library is ",
              "type" : "text"
            },
            {
              "inlineContent" : [
                {
                  "text" : "experimental",
                  "type" : "text"
                }
              ],
              "type" : "strong"
            },
            {
              "text" : " in the current implementation. As we continue to develop the LiveView Native ecosystem this library will likely see rapid evolution and changes.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "anchor" : "Installation",
          "level" : 3,
          "text" : "Installation",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "If ",
              "type" : "text"
            },
            {
              "identifier" : "https:\/\/hex.pm\/docs\/publish",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : ", the package can be installed",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "by adding ",
              "type" : "text"
            },
            {
              "code" : "live_view_native_swift_ui",
              "type" : "codeVoice"
            },
            {
              "text" : " to your list of dependencies in ",
              "type" : "text"
            },
            {
              "code" : "mix.exs",
              "type" : "codeVoice"
            },
            {
              "text" : ":",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "def deps do",
            "  [",
            "    {:live_view_native_swift_ui, \"~> 0.1.0\"}",
            "  ]",
            "end"
          ],
          "syntax" : "elixir",
          "type" : "codeListing"
        },
        {
