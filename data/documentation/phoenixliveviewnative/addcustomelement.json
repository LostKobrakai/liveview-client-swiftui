{
  "abstract": [
    {
      "text": "Use the ",
      "type": "text"
    },
    {
      "identifier": "doc://PhoenixLiveViewNative/documentation/PhoenixLiveViewNative/CustomRegistry",
      "isActive": true,
      "type": "reference"
    },
    {
      "text": " protocol to define how DOM elements are converted to SwiftUI views.",
      "type": "text"
    }
  ],
  "hierarchy": {
