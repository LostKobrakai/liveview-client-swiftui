            {
              "text" : "Create an event to handle the delete action.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "defmodule MyAppWeb.ListLive do",
            "  def handle_event(\"remove_last\", _params, socket):",
            "    {:noreply, assign(socket, :count, socket.assigns.count - 1)}",
            "  end",
            "end"
          ],
          "syntax" : "elixir",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "Pass the name of this event to the ",
              "type" : "text"
            },
            {
              "identifier" : "doc:\/\/LiveViewNative\/documentation\/LiveViewNative\/OnDeleteCommandModifier\/action",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : " argument.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "<List modifiers={@native |> on_delete_command(action: \"remove_last\")}>",
            "  <%= for i <- 0..@count do %>",
            "    <Text id={\"#{i}\"}>",
            "      <%= i %>",
            "    <\/Text>",
            "  <% end %>",
            "<\/List>"
          ],
          "syntax" : "html",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "Whenever backspace or delete is pressed, the number of items in the list is decreased.",
