        {
          "inlineContent": [
            {
              "text": "If the attribute is present, the value is ",
              "type": "text"
            },
            {
              "code": "true",
              "type": "codeVoice"
            },
            {
              "text": ".",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The strings ",
                  "type": "text"
                },
                {
                  "code": "\"true\"",
                  "type": "codeVoice"
                },
                {
                  "text": "/",
                  "type": "text"
                },
                {
                  "code": "\"false\"",
                  "type": "codeVoice"
                },
                {
                  "text": " are ignored, and only the presence of the attribute is considered.",
                  "type": "text"
                },
                {
                  "text": " ",
                  "type": "text"
                },
                {
                  "text": "A value of ",
                  "type": "text"
                },
                {
                  "code": "\"false\"",
                  "type": "codeVoice"
                },
                {
                  "text": " would still return ",
                  "type": "text"
                },
                {
                  "code": "true",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
