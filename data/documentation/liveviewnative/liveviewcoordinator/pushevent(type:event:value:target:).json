      "kind": "parameters",
      "parameters": [
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The type of event that is being sent (e.g., ",
                  "type": "text"
                },
                {
                  "code": "click",
                  "type": "codeVoice"
                },
                {
                  "text": " or ",
                  "type": "text"
                },
                {
                  "code": "form",
                  "type": "codeVoice"
                },
                {
                  "text": "). Note: this is not currently used by the LiveView backend.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "type"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The name of the LiveView event handler that the event is being dispatched to.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "event"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The event value to provide to the backend event handler. The value ",
                  "type": "text"
                },
                {
                  "inlineContent": [
                    {
                      "text": "must",
                      "type": "text"
                    }
                  ],
                  "type": "emphasis"
                },
                {
                  "text": " be  serializable using ",
                  "type": "text"
                },
                {
                  "code": "JSONSerialization",
                  "type": "codeVoice"
                },
                {
                  "text": ".",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "value"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "The value of the ",
                  "type": "text"
                },
                {
                  "code": "phx-target",
                  "type": "codeVoice"
                },
                {
                  "text": " attribute.",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "target"
        }
      ]
    },
    {
      "content": [
        {
          "anchor": "discussion",
          "level": 2,
          "text": "Discussion",
          "type": "heading"
        },
        {
          "inlineContent": [
            {
              "text": "This is an asynchronous function that completes when the server finishes processing the event and returns a response.",
              "type": "text"
            },
            {
              "text": " ",
              "type": "text"
            },
            {
              "text": "The client view tree will be updated automatically when the response is received.",
              "type": "text"
            }
          ],
          "type": "paragraph"
        },
        {
          "content": [
            {
              "inlineContent": [
                {
                  "text": "",
                  "type": "text"
                },
                {
                  "identifier": "doc://LiveViewNative/documentation/LiveViewNative/LiveConnectionError/eventError(_:)",
                  "isActive": true,
                  "type": "reference"
                },
                {
                  "text": " if an error is encountered sending the event or processing it on the backend, ",
                  "type": "text"
                },
                {
                  "code": "CancellationError",
                  "type": "codeVoice"
                },
                {
                  "text": " if the coordinator navigates to a different page while the event is being handled",
                  "type": "text"
                }
              ],
              "type": "paragraph"
            }
          ],
          "name": "Throws",
          "style": "note",
          "type": "aside"
        }
      ],
      "kind": "content"
    }
