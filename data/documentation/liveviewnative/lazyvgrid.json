          "items" : [
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "code" : "fixed(size, spacing, alignment)",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : ": creates a single column that takes up the specified amount of space.",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "code" : "flexible(minimum, maximum, spacing, alignment)",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : ": creates a single column that fills the available space.",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "code" : "adaptive(minimum, maximum, spacing, alignment)",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : ": fills the available space with as many columns as will fit.",
                      "type" : "text"
