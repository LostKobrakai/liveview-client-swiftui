        },
        {
          "inlineContent" : [
            {
              "text" : "Use the ",
              "type" : "text"
            },
            {
              "identifier" : "doc:\/\/LiveViewNative\/documentation\/LiveViewNative\/LazyVGrid\/columns",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : " attribute to configure the presentation of the grid.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "<LazyVGrid",
            "    columns={[",
            "        LiveViewNativeSwiftUi.Types.GridItem.fixed(100),",
            "        LiveViewNativeSwiftUi.Types.GridItem.flexible(),",
            "        LiveViewNativeSwiftUi.Types.GridItem.adaptive(50),",
            "    ] |> Jason.encode!}",
            ">",
            "    <%= for i <- 1..50 do %>",
            "        <Text id={i |> Integer.to_string}><%= i %><\/Text>",
            "    <% end %>",
            "<\/LazyVGrid>"
          ],
          "syntax" : "html",
          "type" : "codeListing"
        },
        {
          "content" : [
