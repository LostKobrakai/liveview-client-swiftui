      "content" : [
        {
          "anchor" : "overview",
          "level" : 2,
          "text" : "Overview",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "This modifier can be used to prevent an ",
              "type" : "text"
            },
            {
              "identifier" : "doc:\/\/LiveViewNative\/documentation\/LiveViewNative\/OnSubmitModifier",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : " modifier on a parent element from triggering when a child element is submitted.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "<Form modifiers={on_submit(@native, action: \"submit\")}>",
            "  <TextField value-binding=\"username\">Username<\/TextField>",
            "  <SecureField value-binding=\"password\">Password<\/SecureField>",
            "  ",
            "  <TextField value-binding=\"tags\" modifiers={submit_scope(@native, is_blocking: true)}>Tags<\/TextField>",
            "<\/Form>"
          ],
          "syntax" : "html",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "In the example above, pressing the submit button when focused on the “Username” or “Password” field will send the ",
              "type" : "text"
            },
            {
              "code" : "submit",
              "type" : "codeVoice"
            },
            {
              "text" : " event.",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "However, the “Tags” field will not send that event when submitted.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "anchor" : "Arguments",
          "level" : 2,
          "text" : "Arguments",
