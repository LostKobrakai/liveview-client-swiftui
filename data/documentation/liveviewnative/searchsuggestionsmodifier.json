            },
            {
              "code" : "suggestions",
              "type" : "codeVoice"
            },
            {
              "text" : " argument.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "Use the ",
              "type" : "text"
            },
            {
              "identifier" : "doc:\/\/LiveViewNative\/documentation\/LiveViewNative\/SearchCompletionModifier",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : " modifier on each child to set the completion content.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "<List",
            "    modifiers={",
            "        @native",
            "            |> searchable(...)",
            "            |> search_suggestions(suggestions: :my_suggestions)",
            "    }",
            ">",
            "    ...",
            "    <Group template={:my_suggestions}>",
            "        <Text modifiers={search_completion(@native, completion: \"text completion\")}>text completion<\/Text>",
            "        <Text modifiers={search_completion(@native, token: \"Custom\")}>Custom Token<\/Text>",
            "    <\/Group>",
            "<List>"
