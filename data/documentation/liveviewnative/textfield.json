    {
      "content" : [
        {
          "anchor" : "overview",
          "level" : 2,
          "text" : "Overview",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "Any children of the field will be used as the label.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "<TextField value-binding=\"first_name\">",
            "    First Name",
            "<\/TextField>"
          ],
          "syntax" : "html",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "You can style the label by using elements inside.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "<TextField value-binding=\"last_name\">",
            "    <Text font-weight=\"bold\" font=\"caption\">Last Name<\/Text>",
            "<\/TextField>"
          ],
          "syntax" : "html",
          "type" : "codeListing"
        },
        {
          "anchor" : "Input-Configuration",
          "level" : 3,
          "text" : "Input Configuration",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "Use modifiers to configure how text is input.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "<TextField",
            "    value-binding=\"value\"",
            "    modifiers={",
            "        @native",
            "        |> autocorrection_disabled(disable: true)",
            "        |> text_input_autocapitalization(autocapitalization: :words)",
            "        |> keyboard_type(type: :web_search)",
            "        |> submit_label(submit_label: :search)",
            "    }",
            ">",
            "    Enter Search Text",
            "<\/TextField>"
          ],
          "syntax" : "html",
          "type" : "codeListing"
        },
        {
          "anchor" : "Formatting-Values",
          "level" : 3,
          "text" : "Formatting Values",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "Use the ",
              "type" : "text"
            },
            {
              "identifier" : "doc:\/\/LiveViewNative\/documentation\/LiveViewNative\/TextField\/format",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : " attribute to input values such as numbers and URLs.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "<VStack>",
            "    <TextField",
            "        value-binding=\"amount\"",
            "        format=\"currency\"",
            "        currency-code=\"usd\"",
            "        modifier={@native |> keyboard_type(type: :decimal_pad)}",
            "    >",
            "        Enter Amount",
            "    <\/TextField>",
            "",
            "    <TextField",
            "        value-binding=\"bank_address\"",
            "        axis=\"vertical\"",
            "    >",
            "        Enter Bank Address",
            "    <\/TextField>",
            "<\/VStack>"
          ],
          "syntax" : "html",
          "type" : "codeListing"
        },
